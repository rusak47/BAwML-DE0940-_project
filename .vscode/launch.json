{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Streamlit Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/venv/bin/streamlit",
            "args": [
                "run",
                "${relativeFile}"
            ],
            "console": "integratedTerminal"
        }
    ]
}